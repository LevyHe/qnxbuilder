{
	"name": "qnxbuilder",
	"displayName": "QnxBuilder",
	"description": "Qnx Builder",
   "version": "0.0.5",
   "publisher": "levy",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:extension.QnxBuild",
        "onCommand:extension.QnxReBuild"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.QnxBuild",
				"title": "Qnx Build"
            },
            {
                "command": "extension.QnxReBuild",
                "title": "Qnx ReBuild"
            }
      ],
      "menus": {
         "explorer/context": [
            {
               "when": "explorerResourceIsFolder",
               "command": "extension.QnxBuild",
               "group": "2_build"
            },
            {
                "when": "explorerResourceIsFolder",
                "command": "extension.QnxReBuild",
                "group": "2_build"
            }
         ]
      }
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
